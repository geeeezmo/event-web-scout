{
  "logging": {
    "level": "INFO",
    "log_file_base_name": "event_web_scout"
  },
  "plugin_entry_points": [
    "core_plugins"
  ],
  "plugin_execution": {
    "timeout_seconds": 3
  },
  "plugin_defaults": {
    "priority": 10,
    "enabled": false,
    "config": {
      "google_service_account_token_file": "google_service_account_token.json",
      "google_calendar_id": "<google_calendar_id>"
    }
  },
  "plugins": [
    {
      "name": "example_html_plugin",
      "priority": 3,
      "enabled": true,
      "config": {
        "base_url": "http://localhost/test-html-plugin",
        "sleep_time": 5
      }
    }
  ]
}
